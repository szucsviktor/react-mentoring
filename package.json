    "expressjs",
    "git",
    "node",
    "webpack",
    "babel"
  ],
  "author": "Viktor Szucs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/szucsviktor/react-mentoring/issues"
  },
  "homepage": "https://github.com/szucsviktor/react-mentoring#readme",
