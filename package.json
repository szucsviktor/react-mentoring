    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "json-loader": "^0.5.7",
    "react-test-renderer": "^16.0.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.8.2"
